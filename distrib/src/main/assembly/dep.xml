<assembly>
    <id>dist</id>
    <formats>
        <format>zip</format>
    </formats>
    <includeBaseDirectory>true</includeBaseDirectory>
    <dependencySets>
        <dependencySet>
            <outputDirectory>/</outputDirectory>
            <unpack>true</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping></outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.client-drivers:*</include>
            </includes>
            <excludes>
                <exclude>org.openqa.selenium.client-drivers:selenium-java-client-driver</exclude>
            </excludes>
        </dependencySet>
        
        <dependencySet>
            <outputDirectory>selenium-server-${version}</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping>${artifactId}.${extension}</outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.server:selenium-server:jar:standalone</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-server-${version}</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping>${artifactId}.${extension}</outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.server:selenium-server-coreless</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-server-${version}/javadoc</outputDirectory>
            <unpack>true</unpack>
            <scope>runtime</scope>
            <includes>
                <include>org.openqa.selenium.server:selenium-server-coreless:jar:javadoc</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-server-${version}</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping>selenium-server-sources.${extension}</outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.server:selenium-server-coreless:jar:sources</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-server-${version}</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping>selenium-server-tests.${extension}</outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.server:selenium-server-coreless:jar:tests</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-server-${version}</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping>selenium-server-test-sources.${extension}</outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.server:selenium-server-coreless:jar:test-sources</include>
            </includes>
        </dependencySet>
        
        
        <dependencySet>
            <outputDirectory>selenium-java-client-driver-${version}</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping>${artifactId}.${extension}</outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.client-drivers:selenium-java-client-driver</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-java-client-driver-${version}/javadoc</outputDirectory>
            <unpack>true</unpack>
            <scope>runtime</scope>
            <includes>
                <include>org.openqa.selenium.client-drivers:selenium-java-client-driver:jar:javadoc</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-java-client-driver-${version}</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping>${artifactId}-sources.${extension}</outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.client-drivers:selenium-java-client-driver:jar:sources</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-java-client-driver-${version}</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping>${artifactId}-tests.${extension}</outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.client-drivers:selenium-java-client-driver:jar:tests</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-java-client-driver-${version}</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping>${artifactId}-test-sources.${extension}</outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.client-drivers:selenium-java-client-driver:jar:test-sources</include>
            </includes>
        </dependencySet>
    </dependencySets>
</assembly>