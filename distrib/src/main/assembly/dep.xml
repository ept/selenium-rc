<assembly>
    <id>dist</id>
    <formats>
        <format>zip</format>
    </formats>
    <includeBaseDirectory>true</includeBaseDirectory>
    <dependencySets>
        <dependencySet>
            <outputDirectory>/</outputDirectory>
            <unpack>true</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping></outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.client-drivers:*</include>
            </includes>
            <excludes>
                <exclude>org.openqa.selenium.client-drivers:selenium-java-client-driver</exclude>
            </excludes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-java-client-driver-${version}</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <includes>
                <include>org.openqa.selenium.client-drivers:selenium-java-client-driver</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-java-client-driver-${version}</outputDirectory>
            <unpack>true</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping>javadoc</outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.client-drivers:selenium-java-client-driver:jar:javadoc</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-java-client-driver-${version}</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping>${artifactId}-${version}-sources.${extension}</outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.client-drivers:selenium-java-client-driver:jar:sources</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-java-client-driver-${version}</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping>${artifactId}-${version}-tests.${extension}</outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.client-drivers:selenium-java-client-driver:jar:tests</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>selenium-java-client-driver-${version}</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <outputFileNameMapping>${artifactId}-${version}-test-sources.${extension}</outputFileNameMapping>
            <includes>
                <include>org.openqa.selenium.client-drivers:selenium-java-client-driver:jar:test-sources</include>
            </includes>
        </dependencySet>
    </dependencySets>
</assembly>