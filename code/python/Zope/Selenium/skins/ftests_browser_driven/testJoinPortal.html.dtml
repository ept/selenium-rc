<body>
<table border="1" cellpadding="1" cellspacing="1">
  <tbody>
    <tr>
      <td rowspan="1" colspan="3">testJoinPortal</td>
    </tr>
    <tr>
      <td>open</td>
      <td>./join_form</td>
      <td>&nbsp;</td>
    </tr>
    <tr>
      <td>verifyTextPresent</td>
      <td>Registration Form</td>
      <td>&nbsp;</td>
    </tr>
    <tr>
      <td>type</td>
      <td>fullname</td>
      <td>Jason Huggins</td>
    </tr>
    <tr>
      <td>type</td>
      <td>username</td>
      <td>jrhuggins12345</td>
    </tr>
    <tr>
      <td>type</td>
      <td>email</td>
      <td>jrhuggins@example.com</td>
    </tr>
    <tr>
      <td>type</td>
      <td>password</td>
      <td>12345</td>
    </tr>
    <tr>
      <td>type</td>
      <td>confirm</td>
      <td>12345</td>
    </tr>
    <tr>
      <td>clickAndWait</td>
      <td>form.button.Register</td>
      <td>&nbsp;</td>
    </tr>
    <tr>
      <td>verifyTextPresent</td>
      <td>You have been registered as a member.</td>
      <td>&nbsp;</td>
